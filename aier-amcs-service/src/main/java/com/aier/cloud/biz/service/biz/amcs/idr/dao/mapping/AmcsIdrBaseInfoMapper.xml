  <select id="getMainByCondForList" resultType="com.aier.cloud.biz.service.biz.amcs.idr.entity.AmcsIdrBaseInfo">
		select
			<include refid="BaseColumnListT" />
		from T_AMCS_IDR_BASE_INFO t 
		 LEFT JOIN T_AMCS_IDR_DICT_TYPE A
   			 ON T.DISEASECODE = A.ID AND A.TYPE_CODE = 'DiseaseName'
		<where>
			<if test="diseasecode != null  and diseasecode != ''">
			 	AND  t.DISEASECODE  in(  ${diseasecode})
 				 
			</if>  
		 	<if test="timeBegin != null ">
				and t.CREATE_DATE <![CDATA[ >= ]]> trunc(#{timeBegin})
			</if> 
			<if test="timeEnd != null ">
				and t.CREATE_DATE <![CDATA[ < ]]> trunc(#{timeEnd}+1)
			</if> 
			<if test="status != null  and status != ''">
				and t.status = #{status,jdbcType=VARCHAR}
			</if> 
			<if test="patientname != null  and patientname != ''">
				and upper(t.patientname) like upper(concat('%', concat(#{patientname}, '%')))
				 
			</if> 
			<if test="idcardcode != null  and idcardcode != ''">
				and upper(t.idcardcode) like upper(concat('%', concat(#{idcardcode}, '%')))
				 
			</if> 
		</where>
		 ORDER BY t.CREATE_DATE DESC

	</select>
